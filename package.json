{
  "name": "jellyfish",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0-rc.0",
    "@material-ui/icons": "^4.2.0",
    "array-equal": "^1.0.0",
    "emotion": "^10.0.9",
    "font-awesome": "^4.7.0",
    "left-pad": "^1.3.0",
    "mongoose": "^5.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-scripts": "3.0.1",
    "resulti": "^2.0.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start:react": "cross-env BROWSER=none react-scripts start",
    "start:electron": "cross-env NODE_ENV=development electron .",
    "start": "concurrently -k -p \"[{name}]\" -n \"🦄 React,⭐️ Electron\" -c \"yellow,magenta\" \"npm run start:react\" \"wait-on http://localhost:3000/ && npm run start:electron\"",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "react-scripts build",
    "build:electron": "cross-env NODE_ENV=production electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@storybook/addon-actions": "^5.1.8",
    "@storybook/addon-links": "^5.1.8",
    "@storybook/addons": "^5.1.8",
    "@storybook/react": "^5.1.8",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^11.5.0",
    "electron-builder": "^20.43.0",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "husky": "^1.3.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.10.0",
    "typescript": "^3.5.2",
    "wait-on": "^3.2.0"
  },
  "homepage": "./",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "build": {
    "appId": "com.electron.jellyfish",
    "mac": {
      "category": "public.app-category.education"
    }
  }
}
